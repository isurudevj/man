{
    "name": "man [context] [keywords...]",
    "description": "Type 'man' plus a search term into the Omnibox to open search in new tab.",
    "version": "1.0",
    "background": {
      "page": "background.html",
      "persistent": false
    },
    "omnibox": { "keyword" : "man" },
    "permissions" : [
        "storage"
    ],
    "manifest_version": 2,
    "browser_action": {
      "default_icon": {
        "16": "images/man_16.png",
        "32": "images/man_32.png"
        }
    },
    "icons": {
      "16": "images/man_16.png",
      "32": "images/man_32.png",
      "48": "images/man_48.png",
      "128": "images/man_128.png"
    }
  }
  